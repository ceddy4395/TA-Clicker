{
  "manifest_version": 2,
  "name": "TA Clicker",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://queue.ewi.tudelft.nl/requests/*"
      ],
      "js": ["content.js"]
    }
  ], 
  "background": ["background.js"], 
  "browser_action": {
    "default_icon": "logo.png"
  }
}